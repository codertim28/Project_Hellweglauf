<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="prohell.prohell.classes.view.SettingsPartialImport">
   <children>
      <HBox alignment="CENTER_LEFT" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onSelectFileBtnClick" text="Datei auswählen" />
            <Label fx:id="fileNameLabel" text="Keine Datei ausgewählt!" />
         </children>
      </HBox>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label text="Spalten in der Datei:" />
            <ListView fx:id="columnPreview" maxWidth="200.0" mouseTransparent="true" prefHeight="200.0" />
         </children>
      </VBox>
      <Label fx:id="statusLabel" />
      <Button fx:id="startBtn" mnemonicParsing="false" text="Importieren" />
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
